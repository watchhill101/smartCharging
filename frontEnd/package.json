{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "description": "智能充电",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "prepare": "husky",
    "new": "taro new",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "cross-env SASS_SILENCE_DEPRECATIONS=legacy-js-api taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "cross-env SASS_SILENCE_DEPRECATIONS=legacy-js-api npm run build:h5 -- --watch",
    "dev:h5:https": "npm run build:h5 -- --watch --https",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:harmony-hybrid": "npm run build:harmony-hybrid -- --watch",
    "generate-icons": "node scripts/generate-tabbar-icons.js",
    "prebuild:weapp": "npm run generate-icons",
    "fix:taro": "node scripts/fix-taro.js"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      "last 3 versions",
      "Android >= 4.1",
      "ios >= 8",
      "> 1%",
      "not dead"
    ]
  },
  "author": "",
